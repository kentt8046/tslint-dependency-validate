{
  "extends": ["tslint-microsoft-contrib", "tslint-config-prettier"],
  "rules": {
    "missing-jsdoc": false,
    "no-relative-imports": false,
    "ordered-imports": false,
    "typedef": false,
    "interface-name": false,
    "export-name": false,
    "curly": [true, "ignore-same-line"],
    "no-any": false,
    "prefer-type-cast": false,
    "variable-name": false
  }
}
