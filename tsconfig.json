{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "lib": ["es2018"],
    "moduleResolution": "node",
    "skipLibCheck": true,
    "importHelpers": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "inlineSourceMap": true
  }
}
